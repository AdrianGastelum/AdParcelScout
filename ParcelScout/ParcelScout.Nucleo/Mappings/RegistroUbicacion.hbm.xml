<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="ParcelScout.Nucleo.Entidades.RegistroUbicacion, ParcelScout.Nucleo" table="registro_ubicacion" lazy="false">
    <id name="Id" type="int">
      <column name="id"/>
      <generator class="identity"/>
    </id>

    <property name="Fecha" type="DateTime">
      <column name="fecha"/>
    </property>
    <property name="Ciudad" type="string">
      <column name="ciudad"/>
    </property>
    <property name="Estado" type="string">
      <column name="estado"/>
    </property>
    <property name="Lon" type="double">
      <column name="longitud"/>
    </property>
    <property name="Lat" type="double">
      <column name="latitud"/>
    </property>

  </class>
</hibernate-mapping>
