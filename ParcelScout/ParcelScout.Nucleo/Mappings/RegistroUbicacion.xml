<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ParcelScout.Nucleo" assembly="ParcelScout.Nucleo">
  <class name="ParcelScout.Nucleo.Entidades.Cliente, ParcelScout.Nucleo" table="cliente" lazy="false">
    <id name="Id" type="int">
      <column name="id"/>
      <generator class="identity"/>
    </id>
    <property name="Fecha" type="DateTime">
      <column name="fecha"/>
    </property>
    <property name="Domicilio" type="string">
      <column name="domicilio"/>
    </property>
    <property name="Ciudad" type="string">
      <column name="ciudad"/>
    </property>
    <property name="Estado" type="string">
      <column name="estado"/>
    </property>
    <property name="CodigoPostal" type="string">
      <column name="codigo_postal"/>
    </property>
    <property name="Lon" type="double">
      <column name="Lon"/>
    </property>
    <property name="Lat" type="double">
      <column name="Lat"/>
    </property>
    
  </class>
</hibernate-mapping>
